    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>96699f5d-5f5c-4c5a-ba78-f479000a3817</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="7.0.2" />
